{
  "version": 2,
  "framework": "remix",
  "buildCommand": "npx remix build",
  "devCommand": "npx remix dev",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "builds": [
    { "src": "build/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "build/index.js" }
  ]
}
